    "eslint-plugin-vue": "^8.6.0",
    "msw": "^0.39.2",
    "msw-storybook-addon": "^1.6.3",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "storybook-vue3-router": "^2.2.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue": "3.2.31",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.34.2"
  },
  "peerDependencies": {
    "@tager/admin-services": "^1.0.0",
    "@tager/admin-ui": "^1.0.0",
    "vue": "^3.2.x",
    "vue-router": "^4.0.x"
